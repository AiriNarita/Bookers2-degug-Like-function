<div class="serch_form" >
      <table align="center">
        <%= form_with url: searches_search_path,method: :get, local: true do |f| %>
        <tr>
          <td><%= f.text_field :content %></td>
          <td><%= f.select :model,options_for_select({"User"=>"user","Book"=>"book"})%></td>
          <td><%= f.select :method,options_for_select({"完全一致"=>"perfect","前方一致"=>"forward",
                "後方一致"=>"backward","部分一致"=>"partial"})%></td>  
          <td><%= f.submit'検索' %></td>
      <% end %>
        </tr>
      </table>
</div>
